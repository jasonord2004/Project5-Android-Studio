<+>package com.example.project4rupizzeria;\n\nimport javafx.collections.FXCollections;\nimport javafx.collections.ObservableList;\nimport javafx.event.ActionEvent;\nimport javafx.event.EventHandler;\nimport javafx.fxml.FXML;\nimport javafx.scene.control.Button;\nimport javafx.scene.control.ComboBox;\nimport javafx.scene.control.ListView;\nimport javafx.scene.control.TextField;\nimport javafx.stage.FileChooser;\n\nimport javax.swing.*;\nimport java.io.File;\nimport java.io.FileWriter;\nimport java.io.IOException;\nimport java.util.ArrayList;\n\npublic class OrdersPlacedController {\n    public static ArrayList<Order> orders = new ArrayList<Order>();\n\n    @FXML\n    private ComboBox orderNumberBox;\n\n    @FXML\n    private TextField orderTotal;\n\n    @FXML\n    private ListView ordersListView;\n\n    @FXML\n    private Button cancelButton;\n\n    @FXML\n    private Button exportButton;\n\n    @FXML\n    void cancelOrder(){\n        if (orders.size() >= 1){\n            int number = Integer.parseInt((String)(orderNumberBox.getValue()));\n            orders.remove(findOrderIndex(number));\n            displayDefault();\n            fillOrderNumberBox();\n        }\n    }\n\n    @FXML\n    void exportOrder(){\n        FileChooser fileChooser = new FileChooser();\n\n        File file = fileChooser.showSaveDialog();\n\n\n        if (file != null){\n            try (FileWriter writer = new FileWriter(file)) {\n                for (Order order : orders){\n                    writer.write(\"Order Number: \" + order.getNumber() + \"\\n\");\n                    for (Pizza pizza : order.getPizzas()){\n                        writer.write(pizza + \"\\n\");\n                    }\n                }\n\n            } catch (IOException e) {\n                e.printStackTrace();\n            }\n        }\n    }\n\n    public void initialize(){\n        fillOrderNumberBox();\n        displayDefault();\n        orderNumberBox.setOnAction(new EventHandler<ActionEvent>() {\n            @Override\n            public void handle(ActionEvent actionEvent) {\n                displayOrderTotal();\n                displayOrder();\n            }\n        });\n    }\n\n    public void displayDefault(){\n        if (orders.size() >= 1){\n            orderNumberBox.setValue(orders.get(0).getNumber());\n            displayOrderTotal();\n            displayOrder();\n        }\n        else{\n            orderTotal.setText(\"0.0\");\n            ordersListView.setItems(null);\n        }\n    }\n\n    private void fillOrderNumberBox(){\n        ArrayList<Integer> orderNumbers = new ArrayList<Integer>();\n        for (Order order : orders){\n            orderNumbers.add(order.getNumber());\n        }\n        orderNumberBox.getItems().addAll(orderNumbers);\n    }\n\n    private void displayOrderTotal() {\n        int orderNumber = Integer.parseInt((String) orderNumberBox.getValue());\n        Order order = findOrder(orderNumber);\n        orderTotal.setText(order.getTotalPrice() + \"\");\n    }\n\n    private void displayOrder(){\n        int orderNumber = Integer.parseInt((String) orderNumberBox.getValue());\n        Order order = findOrder(orderNumber);\n        ObservableList<Pizza> ordersList = FXCollections.observableArrayList(order.getPizzas());\n        ordersListView.setItems(null);\n        ordersListView.setItems(ordersList);\n    }\n\n    private Order findOrder(int number){\n        for (Order order : orders){\n            if (order.getNumber() == number) return order;\n        }\n        return null;\n    }\n\n    private int findOrderIndex(int number){\n        for (int i = 0; i < orders.size(); i++) {\n            if (orders.get(i).getNumber() == number) return i;\n        }\n        return -1;\n    }\n\n}\n
